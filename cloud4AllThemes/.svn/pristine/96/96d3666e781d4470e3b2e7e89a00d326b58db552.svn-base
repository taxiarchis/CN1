package gr.certh.hit.app.map;

public class Poi {

    private String name;
    private String poitype;
    private String street;
    private String streetNumber;
    private String postcode;
    private String telephone;
    private String fax;
    private String url;
    private String email;
    private String city;
    private String coordX;
    private String coordY;
    private String accStatus;
	private String accDescription;

    public Poi()
    {
    	name = "";
    	poitype = "";
    	street = "";
    	streetNumber = "";
    	postcode = "";
    	telephone = "";
    	fax = "";
        url = "";
        email = "";
        city = "";
        coordX = "0";
        coordY = "0";
        accStatus = "";
        accDescription = "";
    }

    public Poi(String theName,String thePoitype, String theStreet, String theStreetNumber, String thePostcode,
                String theTelephone, String theFax, String  theUrl, String theEmail, String theCity, String theCoordX, String theCoordY, String status, String description)
    {
    	name = theName;
    	poitype = thePoitype;
    	street = theStreet;
    	streetNumber = theStreetNumber ;
    	postcode = thePostcode;
    	telephone = theTelephone;
    	fax = theFax;
    	url = theUrl;
    	email = theEmail;
    	city = theCity;
    	coordX = theCoordX;
    	coordY = theCoordY;
        accStatus = status;
        accDescription = description;
    }

    public void setName(String nameOfPOI)
    {
    	name = nameOfPOI;
    }

    public String getName()
    {
        return name;
    }

    public void setPoitype(String nameOfPOI)
    {
    	poitype = nameOfPOI;
    }

    public String getPoitype()
    {
        return poitype;
    }

    public void setStreet(String nameOfPOI)
    {
    	street = nameOfPOI;
    }

    public String getStreet()
    {
        return street;
    }

    public void setStreetNo(String nameOfPOI)
    {
    	streetNumber = nameOfPOI;
    }

    public String getStreetNo()
    {
        return streetNumber;
    }

    public void setPostcode(String nameOfPOI)
    {
        postcode = nameOfPOI;
    }

    public String getPostcode()
    {
        return postcode;
    }

    public void setTelephone(String nameOfPOI)
    {
    	telephone = nameOfPOI;
    }

    public String getTelephone()
    {
        return telephone;
    }

    public void setFax(String nameOfPOI)
    {
    	fax = nameOfPOI;
    }

    public String getFax()
    {
        return fax;
    }

    public void setUrl(String nameOfPOI)
    {
        url = nameOfPOI;
    }

    public String getUrl()
    {
        return url;
    }

    public void setEmail(String nameOfPOI)
    {
    	email = nameOfPOI;
    }

    public String getEmail()
    {
        return email;
    }

    public void setCity(String nameOfPOI)
    {
    	city = nameOfPOI;
    }

    public String getCity()
    {
        return city;
    }

    public void setCoordX(String coord_x)
    {
    	coordX = coord_x;
    }

    public String getCoordX()
    {
        return coordX;
    }

    public void setCoordY(String coord_y)
    {
    	coordY = coord_y;
    }

    public String getCoordY()
    {
        return coordY;
    }
    
    public String getAccStatus() {
		return accStatus;
	}

	public void setAccStatus(String accStatus) {
		this.accStatus = accStatus;
	}

	public String getAccDescription() {
		return accDescription;
	}

	public void setAccDescription(String accDescription) {
		this.accDescription = accDescription;
	}
}
